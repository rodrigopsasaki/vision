{
  "name": "@rodrigopsasaki/datadog-exporter",
  "type": "module",
  "version": "0.1.0",
  "description": "Comprehensive Datadog exporter for structured observability data",
  "license": "MIT",
  "author": "Rodrigo Sasaki",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rodrigopsasaki/vision.git"
  },
  "bugs": {
    "url": "https://github.com/rodrigopsasaki/vision/issues"
  },
  "homepage": "https://github.com/rodrigopsasaki/vision#readme",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint src test --ext .ts",
    "lint:fix": "eslint src test --ext .ts --fix",
    "format": "prettier --config ../../.prettierrc --ignore-path ../../.prettierignore --write .",
    "format:check": "prettier --config ../../.prettierrc --ignore-path ../../.prettierignore --check .",
    "spellcheck": "cspell '**/*.{ts,md,json}'",
    "build": "tsup src/index.ts --format esm --dts --out-dir dist",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest run",
    "test:dev": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "datadog",
    "exporter",
    "observability",
    "metrics",
    "logs",
    "traces",
    "monitoring",
    "typescript",
    "nodejs"
  ],
  "dependencies": {
    "@datadog/browser-logs": "^6.0.0",
    "@datadog/browser-rum": "^5.0.0",
    "node-fetch": "^3.3.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cspell": "^9.1.5",
    "eslint": "^9.31.0",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@rodrigopsasaki/vision": "workspace:*"
  }
} 